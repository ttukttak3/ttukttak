{
    "plugins": [
      "prettier"
    ],
    
    "extends": [
      "airbnb",
      "plugin:prettier/recommended"
    ],
    
    "env": {
      "browser": true,
      "node": true
    },
    
    "rules": {
      "no-new": "off",
      "no-console": "off",
      "no-param-reassign": "off",
      "no-alert": "off",
      "no-undef": "off",
      "import/extensions": "off",
      "prefer-template": "off",
      "max-depth": ["error", 2],
      "max-lines-per-function": ["error", 15],
      "eol-last": ["error", "always"],
      "no-unused-vars": 1,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
  }